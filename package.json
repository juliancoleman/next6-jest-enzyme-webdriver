    "@types/chromedriver": "^2.38.0",
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.3",
    "@types/next": "^2.4.11",
    "@types/node": "^10.1.2",
    "@types/ramda": "^0.25.29",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/selenium-webdriver": "^3.0.8",
    "@types/styled-jsx": "^2.2.3",
    "@types/webdriverio": "^4.10.1",
    "@zeit/next-css": "^0.2.0",
